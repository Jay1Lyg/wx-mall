<!--pages/classresults/classresults.wxml-->
<import src="../template/template.wxml"/> 
<view class='tab'>
	<view class="tabdetail " bindtap='price'>
		<text>价格</text>
	</view>
	<view class="tabdetail borders " bindtap='sales'>
		<text>销量</text>
	</view>
	<view class="tabdetail {{active?'active':''}}" bindtap='newproduce'>
		<text>上新</text>
	</view>
</view>
<view class='recommend-detail'>
		<template is='detail' data='{{recommend}}' >
		</template>
</view>
<view style='clear:both'></view>
